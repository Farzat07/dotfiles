# vim: set filetype=sshconfig :

Host ubuntu1
    HostName ubuntu1.farzat.xyz
    User farzat
    ForwardAgent yes
    IdentitiesOnly yes
    IdentityFile ${XDG_DATA_HOME}/ssh_pub_keys/ubuntu1.pub

Host arch1
    HostName arch1.farzat.xyz
    User farzat
    ForwardAgent yes
    IdentitiesOnly yes
    IdentityFile ${XDG_DATA_HOME}/ssh_pub_keys/arch1.pub
